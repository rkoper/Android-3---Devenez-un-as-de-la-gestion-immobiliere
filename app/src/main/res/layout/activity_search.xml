<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    android:background="@color/colorD"
    android:orientation="vertical"
    tools:context=".controller.activity.SearchActivity">
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/a_search_fb_home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="1dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:backgroundTint="@color/colorA"
        android:scaleType="centerCrop"
        android:src="@drawable/house_grey"
        app:maxImageSize="36dp"
        app:tint="@color/colorD"
        tools:targetApi="lollipop" />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:background="@color/colorB"
        android:layout_marginBottom="20dp"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.93" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.0" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.09" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.18" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline3A"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.26" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline3date1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.3" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline3date2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.36" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.48" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.57" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline6A"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.63" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.73" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.78" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.83" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineV1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.1" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineV2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.9" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.38" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineC2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.62" />


        <LinearLayout
            android:id="@+id/a_search_fb_searchicon"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/guideline"
            app:layout_constraintTop_toBottomOf="@+id/a_search_cb_park"
            android:background="@drawable/round_yellow"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_weight="3"
                android:layout_gravity="center"
                android:id="@+id/clickSearch"
                android:src="@drawable/eye_grey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_gravity="center"
                style="@style/MyEditTextStyleSearch"
                android:id="@+id/a_search_txt_item"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1"
                android:textColor="@color/colorBlack"
                android:layout_weight="4"
                />
        </LinearLayout>

        <com.appyvet.materialrangebar.RangeBar
            app:layout_constraintTop_toTopOf="@+id/guideline1"
            app:layout_constraintLeft_toLeftOf="@+id/guidelineV1"
            app:layout_constraintRight_toRightOf="@+id/guidelineV2"
            app:layout_constraintBottom_toTopOf="@+id/guideline2"
            android:id="@+id/a_search_rangebar_price"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:mrb_pinRadius="0dp"
            app:mrb_rangeBarPaddingBottom="40dp"
            app:mrb_connectingLineColor="@color/colorD"
            app:mrb_rightThumbColor="@color/colorD"
            app:mrb_leftThumbColor="@color/colorD"
            app:mrb_rangeBarColor="@color/colorB"
            app:mrb_thumbSize="10dp"
            app:mrb_tickEnd="300"
            app:mrb_tickInterval="5"
            app:mrb_tickStart="10" />


        <TextView
            android:gravity="bottom"
            style="@style/MyEditTextStyleSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/a_search_rangebar_price"
            app:layout_constraintLeft_toLeftOf="@+id/a_search_rangebar_price"
            app:layout_constraintRight_toRightOf="@+id/a_search_rangebar_price"
            android:text="Price"/>
        <TextView
            android:id="@+id/a_search_tx_pricemini"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/a_search_rangebar_price"
            app:layout_constraintLeft_toLeftOf="@+id/a_search_rangebar_price"
            android:text="mini"/>
        <TextView
            android:id="@+id/a_search_tx_pricemax"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/a_search_rangebar_price"
            app:layout_constraintRight_toRightOf="@+id/a_search_rangebar_price"
            android:text="max"/>

        <com.appyvet.materialrangebar.RangeBar
            app:layout_constraintTop_toBottomOf="@+id/guideline2"
            app:layout_constraintLeft_toLeftOf="@+id/guidelineV1"
            app:layout_constraintRight_toRightOf="@+id/guidelineV2"
            app:layout_constraintBottom_toTopOf="@id/guideline3"
            android:id="@+id/a_search_rangebar_surface"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:mrb_rangeBarPaddingBottom="40dp"
            app:mrb_connectingLineColor="@color/colorD"
            app:mrb_rightThumbColor="@color/colorD"
            app:mrb_leftThumbColor="@color/colorD"
            app:mrb_rangeBarColor="@color/colorB"
            app:mrb_thumbSize="10dp"
            app:mrb_tickEnd="3000"
            app:mrb_pinRadius="0dp"
            app:mrb_tickInterval="50"
            app:mrb_tickStart="100" />
        <TextView
            android:gravity="bottom"
            style="@style/MyEditTextStyleSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/guideline3"
            app:layout_constraintLeft_toLeftOf="@+id/a_search_rangebar_surface"
            app:layout_constraintRight_toRightOf="@+id/a_search_rangebar_surface"
            android:text="Surface"/>
        <TextView
            android:id="@+id/a_search_tx_minisurface"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/a_search_rangebar_surface"
            app:layout_constraintLeft_toLeftOf="@+id/a_search_rangebar_surface"
            android:text="mini "/>
        <TextView
            android:id="@+id/a_search_tx_maxsurface"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/a_search_rangebar_surface"
            app:layout_constraintRight_toRightOf="@+id/a_search_rangebar_surface"
            android:text="max"/>



        <TextView
            app:layout_constraintRight_toLeftOf="@+id/guidelineC1"
            android:id="@+id/txtCreateDate1"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="Create between  :"
            android:textColorHint="@color/colorD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/guideline3date1"
            app:layout_constraintTop_toBottomOf="@+id/guideline3"
            android:importantForAutofill="no" />

        <TextView
            app:layout_constraintRight_toLeftOf="@+id/cancelDCB"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="Date -"

            android:textColorHint="@color/colorD"
            android:id="@+id/a_search_ed_dateBegin_create"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/guideline3date1"
            app:layout_constraintLeft_toRightOf="@+id/txtCreateDate1"
            app:layout_constraintTop_toBottomOf="@+id/guideline3"
            android:importantForAutofill="no" />
        <ImageButton
            app:layout_constraintRight_toLeftOf="@+id/txtCreateDate2"
            android:visibility="gone"
            android:id="@+id/cancelDCB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/guideline3date1"
            android:background="@drawable/cancel_yellow_small"
            app:layout_constraintLeft_toRightOf="@+id/a_search_ed_dateBegin_create"
            app:layout_constraintTop_toBottomOf="@+id/guideline3"/>
        <TextView
            app:layout_constraintRight_toLeftOf="@+id/a_search_ed_dateEnd_create"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="&amp;"
            android:id="@+id/txtCreateDate2"
            app:layout_constraintBottom_toTopOf="@+id/guideline3date1"
            android:textColorHint="@color/colorD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@+id/cancelDCB"
            app:layout_constraintTop_toBottomOf="@+id/guideline3"
            android:importantForAutofill="no" />
        <TextView
            app:layout_constraintRight_toLeftOf="@+id/cancelDCE"
            android:gravity="right"
            android:textColorHint="@color/colorD"
            style="@style/MyEditTextStyleSearch"
            android:hint="Date +"
            app:layout_constraintBottom_toTopOf="@+id/guideline3date1"
            android:background="@color/colorB"
            android:id="@+id/a_search_ed_dateEnd_create"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@+id/txtCreateDate2"
            app:layout_constraintTop_toBottomOf="@+id/guideline3"
            android:importantForAutofill="no" />

        <ImageButton
            app:layout_constraintBottom_toTopOf="@+id/guideline3date1"
            app:layout_constraintRight_toLeftOf="@+id/guidelineV2"
            android:visibility="gone"
            android:id="@+id/cancelDCE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/cancel_yellow_small"
            app:layout_constraintLeft_toRightOf="@+id/a_search_ed_dateEnd_create"
            app:layout_constraintTop_toBottomOf="@+id/guideline3"/>


        <TextView
            app:layout_constraintTop_toBottomOf="@+id/guideline3date2"
            android:id="@+id/txtSoldDate1"
            android:gravity="center"
            android:visibility="invisible"
            style="@style/MyEditTextStyleSearch"
            android:hint="Sold between  :"
            android:textColorHint="@color/colorD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/guideline4"
            app:layout_constraintRight_toLeftOf="@+id/guidelineC1"
            android:importantForAutofill="no" />

        <TextView
            app:layout_constraintTop_toBottomOf="@+id/guideline3date2"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"

            android:background="@color/colorB"
            android:hint="Date -"
            android:visibility="invisible"
            android:textColorHint="@color/colorD"
            android:id="@+id/a_search_sold_dateBegin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@+id/txtSoldDate1"
            app:layout_constraintRight_toLeftOf="@+id/cancelDSB"
            app:layout_constraintBottom_toBottomOf="@+id/guideline4"/>

        <ImageButton
            app:layout_constraintTop_toBottomOf="@+id/guideline3date2"
            android:visibility="gone"
            android:id="@+id/cancelDSB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/cancel_yellow_small"
            app:layout_constraintLeft_toRightOf="@+id/a_search_sold_dateBegin"
            app:layout_constraintRight_toLeftOf="@+id/txtSoldDate2"
            app:layout_constraintBottom_toBottomOf="@+id/guideline4"/>
        <TextView
            app:layout_constraintTop_toBottomOf="@+id/guideline3date2"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="&amp;"
            android:id="@+id/txtSoldDate2"
            android:visibility="invisible"

            android:textColorHint="@color/colorD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@+id/cancelDSB"
            app:layout_constraintBottom_toBottomOf="@+id/guideline4"
            app:layout_constraintRight_toLeftOf="@+id/a_search_sold_dateEnd"
            android:importantForAutofill="no" />
        <TextView
            app:layout_constraintTop_toBottomOf="@+id/guideline3date2"
            android:gravity="center"
            android:visibility="invisible"
            style="@style/MyEditTextStyleSearch"
            android:id="@+id/a_search_sold_dateEnd"
            android:textColorHint="@color/colorD"
            android:layout_width="wrap_content"
            android:text="Date +"
            android:layout_height="wrap_content"

            app:layout_constraintLeft_toRightOf="@+id/txtSoldDate2"
            app:layout_constraintBottom_toBottomOf="@+id/guideline4"
            app:layout_constraintRight_toLeftOf="@+id/cancelDSE"
            android:importantForAutofill="no" />
        <ImageButton
            app:layout_constraintTop_toBottomOf="@+id/guideline3date2"
            android:visibility="gone"
            android:id="@+id/cancelDSE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/cancel_yellow_small"
            app:layout_constraintLeft_toRightOf="@+id/a_search_sold_dateEnd"
            app:layout_constraintBottom_toBottomOf="@+id/guideline4"
            app:layout_constraintRight_toLeftOf="@+id/guidelineV2"/>

        <Spinner
            style="@style/spinnerDropDownItemStyle"
            android:id="@+id/spPersonn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/guidelineV1"
            app:layout_constraintBottom_toTopOf="@+id/guideline4"
            app:layout_constraintRight_toRightOf="@id/guidelineC1"
            app:layout_constraintTop_toBottomOf="@id/guideline4"/>

        <Spinner
            style="@style/spinnerDropDownItemStyle"
            android:id="@+id/spType"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintLeft_toRightOf="@id/guidelineC2"
            app:layout_constraintRight_toRightOf="@+id/guidelineV2"
            app:layout_constraintBottom_toTopOf="@+id/guideline4"
            app:layout_constraintTop_toBottomOf="@id/guideline4"
            android:importantForAutofill="no" />

        <LinearLayout
            android:id="@+id/a_search_cb_sold"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintLeft_toRightOf="@+id/guidelineC1"
            app:layout_constraintRight_toLeftOf="@id/guidelineC2"
            app:layout_constraintTop_toBottomOf="@id/guideline3date1"
            app:layout_constraintBottom_toBottomOf="@+id/guideline3date2"
            android:orientation="horizontal">

            <androidx.appcompat.widget.SwitchCompat
                android:layout_weight="3"
                android:theme="@style/ThemeOverlay.MySwitchCompat"
                android:id="@+id/search_switch_sold"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
            <TextView
                android:layout_weight="3"
                android:id="@+id/TxtSold"
                android:gravity="center"
                style="@style/MyEditTextStyleSearch"
                android:text="Sold"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
        </LinearLayout>
        <TextView
            android:gravity="bottom"
            style="@style/MyEditTextStyleSearch"
            android:text="Room"
            android:background="@color/colorB"
            android:id="@+id/a_search_ed_room"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/a_search_ed_photo"
            app:layout_constraintTop_toBottomOf="@id/guideline4"
            app:layout_constraintBottom_toTopOf="@+id/guideline5"
            android:importantForAutofill="no" />
        <TextView
            android:gravity="bottom"
            style="@style/MyEditTextStyleSearch"
            android:text="0"
            android:background="@color/colorB"
            android:id="@+id/a_search_ed_room_mini"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@+id/guidelineV1"
            app:layout_constraintBottom_toBottomOf="@id/a_search_ed_room_rangeBar"
            android:importantForAutofill="no" />
        <TextView
            android:gravity="bottom"
            style="@style/MyEditTextStyleSearch"
            android:text="+"
            android:background="@color/colorB"
            android:id="@+id/a_search_ed_room_max"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintRight_toRightOf="@id/guidelineC1"
            app:layout_constraintBottom_toBottomOf="@id/a_search_ed_room_rangeBar"
            android:importantForAutofill="no" />
        <com.appyvet.materialrangebar.RangeBar
            app:layout_constraintTop_toTopOf="@+id/a_search_ed_room"
            app:layout_constraintLeft_toLeftOf="@+id/a_search_ed_room"
            app:layout_constraintRight_toRightOf="@+id/a_search_ed_room"
            app:layout_constraintBottom_toBottomOf="@id/a_search_ed_room"
            android:id="@+id/a_search_ed_room_rangeBar"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            app:mrb_rangeBarPaddingBottom="40dp"
            app:mrb_connectingLineColor="@color/colorD"
            app:mrb_rightThumbColor="@color/colorD"
            app:mrb_leftThumbColor="@color/colorD"
            app:mrb_rangeBarColor="@color/colorB"
            app:mrb_thumbSize="10dp"
            app:mrb_tickEnd="12"
            app:mrb_pinRadius="0dp"
            app:mrb_tickInterval="1"
            app:mrb_tickStart="1" />
        <TextView
            android:gravity="bottom"
            style="@style/MyEditTextStyleSearch"
            android:text="Photo "
            android:background="@color/colorB"
            android:id="@+id/a_search_ed_photo"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline5"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toRightOf="@id/a_search_ed_room"
            app:layout_constraintTop_toBottomOf="@id/guideline4"
            android:importantForAutofill="no" />

        <TextView
            android:gravity="bottom"
            style="@style/MyEditTextStyleSearch"
            android:text="0"
            android:background="@color/colorB"
            android:id="@+id/a_search_ed_photo_mini"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@+id/guidelineC2"
            app:layout_constraintBottom_toBottomOf="@id/a_search_ed_photo_rangebar"
            android:importantForAutofill="no" />
        <TextView
            android:gravity="bottom"
            style="@style/MyEditTextStyleSearch"
            android:text="+"
            android:background="@color/colorB"
            android:id="@+id/a_search_ed_photo_max"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintRight_toRightOf="@id/guidelineV2"
            app:layout_constraintBottom_toBottomOf="@id/a_search_ed_photo_rangebar"
            android:importantForAutofill="no" />
        <com.appyvet.materialrangebar.RangeBar
            app:layout_constraintTop_toTopOf="@+id/a_search_ed_photo"
            app:layout_constraintLeft_toLeftOf="@+id/a_search_ed_photo"
            app:layout_constraintRight_toRightOf="@+id/a_search_ed_photo"
            app:layout_constraintBottom_toBottomOf="@id/a_search_ed_photo"
            android:id="@+id/a_search_ed_photo_rangebar"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            app:mrb_rangeBarPaddingBottom="40dp"
            app:mrb_connectingLineColor="@color/colorD"
            app:mrb_rightThumbColor="@color/colorD"
            app:mrb_leftThumbColor="@color/colorD"
            app:mrb_rangeBarColor="@color/colorB"
            app:mrb_thumbSize="10dp"
            app:mrb_tickEnd="19"
            app:mrb_pinRadius="0dp"
            app:mrb_tickInterval="1"
            app:mrb_tickStart="0" />

        <fragment
            android:id="@+id/autocomplete_fragment"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="10dp"
            android:name="com.google.android.libraries.places.widget.AutocompleteSupportFragment"
            app:layout_constraintTop_toTopOf="@+id/guideline6A"
            app:layout_constraintBottom_toBottomOf="@+id/guideline6"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent" />

        <TextView
            android:layout_margin="10dp"
            android:backgroundTint="@color/colorB"
            style="@style/MyEditTextStyleSearch"
