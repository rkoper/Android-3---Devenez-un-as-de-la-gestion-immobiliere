<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    android:background="@color/colorD"
    android:orientation="vertical"
    tools:context=".controller.activity.SearchActivity">
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/a_search_fb_home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="1dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:backgroundTint="@color/colorA"
        android:scaleType="centerCrop"
        android:src="@drawable/house_grey"
        app:maxImageSize="36dp"
        app:tint="@color/colorD"
        tools:targetApi="lollipop" />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:background="@color/colorB"
        android:layout_marginBottom="20dp"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.93" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.84" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.77" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.7" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.66" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.56" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.52" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.42" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.37" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.35" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.25" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.2" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.18" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.08" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/ddguidelineX17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.03" />
        
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineV1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.1" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineV2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.9" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineV3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.38" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineV4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.62" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineCenter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />


        <LinearLayout
            android:id="@+id/a_search_fb_searchicon"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ddguidelineX2"
            android:background="@drawable/round_yellow"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_weight="3"
                android:layout_gravity="center"
                android:id="@+id/clickSearch"
                android:src="@drawable/eye_grey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_gravity="center"
                style="@style/MyEditTextStyleSearch"
                android:id="@+id/a_search_txt_item"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1"
                android:textColor="@color/colorBlack"
                android:layout_weight="4"
                />
        </LinearLayout>

        <com.appyvet.materialrangebar.RangeBar
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX16"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="@+id/guidelineCenter"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX15"
            android:id="@+id/a_search_rangebar_price"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:mrb_pinRadius="0dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:mrb_rangeBarPaddingBottom="40dp"
            app:mrb_connectingLineColor="@color/colorD"
            app:mrb_rightThumbColor="@color/colorD"
            app:mrb_leftThumbColor="@color/colorD"
            app:mrb_rangeBarColor="@color/colorB"
            app:mrb_thumbSize="10dp"
            app:mrb_tickEnd="300"
            app:mrb_tickInterval="5"
            app:mrb_tickStart="10" />


        <TextView
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:background="@color/colorMix"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX17"
            app:layout_constraintBottom_toBottomOf="@+id/ddguidelineX16"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="@+id/guidelineCenter"
            android:text="Price"/>
        <TextView
            android:id="@+id/a_search_tx_pricemini"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/a_search_rangebar_price"
            app:layout_constraintLeft_toLeftOf="@+id/a_search_rangebar_price"
            android:text="mini"/>
        <TextView
            android:id="@+id/a_search_tx_pricemax"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/a_search_rangebar_price"
            app:layout_constraintRight_toRightOf="@+id/a_search_rangebar_price"
            android:text="max"/>

        <com.appyvet.materialrangebar.RangeBar
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX16"
            app:layout_constraintLeft_toLeftOf="@id/guidelineCenter"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX15"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:id="@+id/a_search_rangebar_surface"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:mrb_rangeBarPaddingBottom="40dp"
            app:mrb_connectingLineColor="@color/colorD"
            app:mrb_rightThumbColor="@color/colorD"
            app:mrb_leftThumbColor="@color/colorD"
            app:mrb_rangeBarColor="@color/colorB"
            app:mrb_thumbSize="10dp"
            app:mrb_tickEnd="3000"
            app:mrb_pinRadius="0dp"
            app:mrb_tickInterval="50"
            app:mrb_tickStart="100" />
        <TextView
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX17"
            app:layout_constraintBottom_toBottomOf="@+id/ddguidelineX16"
            android:background="@color/colorMix"
            app:layout_constraintLeft_toLeftOf="@+id/guidelineCenter"
            app:layout_constraintRight_toRightOf="parent"
            android:text="Surface"/>
        <TextView
            android:id="@+id/a_search_tx_minisurface"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/a_search_rangebar_surface"
            app:layout_constraintLeft_toLeftOf="@+id/a_search_rangebar_surface"
            android:text="mini "/>
        <TextView
            android:id="@+id/a_search_tx_maxsurface"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/a_search_rangebar_surface"
            app:layout_constraintRight_toRightOf="@+id/a_search_rangebar_surface"
            android:text="max"/>



        <TextView
            android:id="@+id/txtCreateDate1"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="Create between  :"
            android:textColorHint="@color/colorD"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@color/colorMix"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX12"
            app:layout_constraintTop_toBottomOf="@+id/ddguidelineX13"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="@id/guidelineCenter"
            android:importantForAutofill="no" />

        <TextView
            app:layout_constraintRight_toLeftOf="@+id/cancelDCB"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="Date -"
            android:textColorHint="@color/colorD"
            android:id="@+id/a_search_ed_dateBegin_create"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX12"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX11"
            app:layout_constraintLeft_toLeftOf="parent"
            android:importantForAutofill="no" />
        <ImageButton
            app:layout_constraintRight_toLeftOf="@+id/txtCreateDate2"
            android:visibility="gone"
            android:id="@+id/cancelDCB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX11"
            android:background="@drawable/cancel_yellow_small"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX12"
            app:layout_constraintLeft_toRightOf="@+id/a_search_ed_dateBegin_create" />
        <TextView
            app:layout_constraintRight_toLeftOf="@+id/a_search_ed_dateEnd_create"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="&amp;"
            android:id="@+id/txtCreateDate2"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX11"
            android:textColorHint="@color/colorD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX12"
            app:layout_constraintLeft_toRightOf="@+id/cancelDCB"
            android:importantForAutofill="no" />
        <TextView
            app:layout_constraintRight_toLeftOf="@+id/cancelDCE"
            android:gravity="right"
            android:textColorHint="@color/colorD"
            style="@style/MyEditTextStyleSearch"
            android:hint="Date +"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX11"
            android:background="@color/colorB"
            android:id="@+id/a_search_ed_dateEnd_create"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX12"
            app:layout_constraintLeft_toRightOf="@+id/txtCreateDate2"
            android:importantForAutofill="no" />

        <ImageButton
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX11"
            app:layout_constraintRight_toLeftOf="@+id/guidelineCenter"
            android:visibility="gone"
            android:id="@+id/cancelDCE"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/cancel_yellow_small"
            app:layout_constraintLeft_toRightOf="@+id/a_search_ed_dateEnd_create" />


        <TextView
            android:id="@+id/txtSoldDate1"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="Sold between  :"
            android:textColorHint="@color/colorD"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@color/colorMix"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX12"
            app:layout_constraintTop_toBottomOf="@+id/ddguidelineX13"
            app:layout_constraintLeft_toLeftOf="@id/guidelineCenter"
            app:layout_constraintRight_toRightOf="parent"
            android:importantForAutofill="no" />

        <TextView
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:background="@color/colorB"
            android:hint="Date -"
            android:textColorHint="@color/colorD"
            android:id="@+id/a_search_sold_dateBegin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@+id/guidelineCenter"
            app:layout_constraintRight_toLeftOf="@+id/cancelDSB"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX12"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX11"
            />

        <ImageButton
            android:visibility="gone"
            android:id="@+id/cancelDSB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/cancel_yellow_small"
            app:layout_constraintLeft_toRightOf="@+id/a_search_sold_dateBegin"
            app:layout_constraintRight_toLeftOf="@+id/txtSoldDate2"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX12"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX11"            />
        <TextView
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="&amp;"
            android:id="@+id/txtSoldDate2"
            android:textColorHint="@color/colorD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX12"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX11"
            app:layout_constraintLeft_toRightOf="@+id/cancelDSB"
            app:layout_constraintRight_toLeftOf="@+id/a_search_sold_dateEnd"
            android:importantForAutofill="no" />
        <TextView
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:id="@+id/a_search_sold_dateEnd"
            android:textColorHint="@color/colorD"
            android:layout_width="wrap_content"
            android:text="Date +"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX12"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX11"
            app:layout_constraintLeft_toRightOf="@+id/txtSoldDate2"
            app:layout_constraintRight_toLeftOf="@+id/cancelDSE"
            android:importantForAutofill="no" />
        <ImageButton
            android:visibility="gone"
            android:id="@+id/cancelDSE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/cancel_yellow_small"
            app:layout_constraintLeft_toRightOf="@+id/a_search_sold_dateEnd"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX12"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX11"
            app:layout_constraintRight_toRightOf="parent"/>
        <TextView
            android:id="@+id/txtPersonn"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="Person"
            android:textColorHint="@color/colorD"
            android:layout_width="0dp"
            android:background="@color/colorMix"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX6"
            app:layout_constraintTop_toBottomOf="@+id/ddguidelineX7"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="@+id/guidelineCenter"
            android:importantForAutofill="no" />
        <TextView
            android:id="@+id/TxtType"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="Type"
            android:textColorHint="@color/colorD"
            android:layout_width="0dp"
            android:background="@color/colorMix"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX6"
            app:layout_constraintTop_toBottomOf="@+id/ddguidelineX7"
            app:layout_constraintLeft_toLeftOf="@+id/guidelineCenter"
            app:layout_constraintRight_toRightOf="parent"
            android:importantForAutofill="no" />
        <Spinner
            style="@style/spinnerDropDownItemStyle"
            android:id="@+id/spPersonn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX6"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX5"
            app:layout_constraintRight_toRightOf="@id/guidelineV3" />

        <Spinner
            style="@style/spinnerDropDownItemStyle"
            android:id="@+id/spType"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/ddguidelineX6"
            app:layout_constraintRight_toRightOf="@+id/guidelineV2"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX5"
            android:importantForAutofill="no" />
        <TextView
            android:id="@+id/txtRoom"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="Room"
            android:textColorHint="@color/colorD"
            android:layout_width="0dp"
            android:background="@color/colorMix"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX8"
            app:layout_constraintTop_toBottomOf="@+id/ddguidelineX9"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="@+id/guidelineCenter"
            android:importantForAutofill="no" />
        <TextView
            android:id="@+id/txtPhoto"
            android:gravity="center"
            style="@style/MyEditTextStyleSearch"
            android:hint="Photo"
            android:textColorHint="@color/colorD"
            android:layout_width="0dp"
            android:background="@color/colorMix"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/ddguidelineX8"
            app:layout_constraintTop_toBottomOf="@+id/ddguidelineX9"
            app:layout_constraintLeft_toLeftOf="@+id/guidelineCenter"
            app:layout_constraintRight_toRightOf="parent"
            android:importantForAutofill="no" />
        <TextView
            android:gravity="bottom"
            style="@style/MyEditTextStyleSearch"
            android:text="0"
            android:background="@color/colorB"
            android:id="@+id/a_search_ed_room_mini"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@+id/guidelineV1"
            app:layout_constraintBottom_toBottomOf="@id/a_search_ed_room_rangeBar"
            android:importantForAutofill="no" />
        <TextView
            android:gravity="bottom"
            style="@style/MyEditTextStyleSearch"
            android:text="+"
            android:background="@color/colorB"
            android:id="@+id/a_search_ed_room_max"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintRight_toRightOf="@id/guidelineV3"
            app:layout_constraintBottom_toBottomOf="@id/a_search_ed_room_rangeBar"
            android:importantForAutofill="no" />
        <com.appyvet.materialrangebar.RangeBar
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX8"
            app:layout_constraintBottom_toBottomOf="@id/ddguidelineX7"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="@+id/guidelineCenter"
            android:id="@+id/a_search_ed_room_rangeBar"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:mrb_rangeBarPaddingBottom="35dp"
            app:mrb_connectingLineColor="@color/colorD"
            app:mrb_rightThumbColor="@color/colorD"
            app:mrb_leftThumbColor="@color/colorD"
            app:mrb_rangeBarColor="@color/colorB"
            app:mrb_thumbSize="10dp"
            app:mrb_tickEnd="12"
            app:mrb_pinRadius="0dp"
            app:mrb_tickInterval="1"
            app:mrb_tickStart="1" />

        <TextView
            android:gravity="bottom"
            style="@style/MyEditTextStyleSearch"
            android:text="0"
            android:background="@color/colorB"
            android:id="@+id/a_search_ed_photo_mini"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@+id/guidelineV4"
            app:layout_constraintBottom_toBottomOf="@id/a_search_ed_photo_rangebar"
            android:importantForAutofill="no" />
        <TextView
            android:gravity="bottom"
            style="@style/MyEditTextStyleSearch"
            android:text="+"
