            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintRight_toRightOf="@id/guidelineV2"
            app:layout_constraintBottom_toBottomOf="@id/a_search_ed_photo_rangebar"
            android:importantForAutofill="no" />
        <com.appyvet.materialrangebar.RangeBar
            app:layout_constraintTop_toTopOf="@+id/ddguidelineX8"
            app:layout_constraintBottom_toBottomOf="@id/ddguidelineX7"
            app:layout_constraintLeft_toLeftOf="@+id/guidelineCenter"
            app:layout_constraintRight_toRightOf="parent"
            android:id="@+id/a_search_ed_photo_rangebar"
            android:layout_width="0dp"
            android:layout_height="0dp"
