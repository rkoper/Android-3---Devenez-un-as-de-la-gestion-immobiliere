        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/detail_guideline9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.88" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/detail_guideline34"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.35" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/detail_guideline35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.65" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/detail_guideline99"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/colorMix"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline2"
            app:layout_constraintBottom_toBottomOf="@+id/detail_guideline2"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/colorMix"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline3"
            app:layout_constraintBottom_toBottomOf="@+id/detail_guideline3"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/colorMix"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline711"
            app:layout_constraintBottom_toBottomOf="@+id/detail_guideline711"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/colorMix"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline4"
            app:layout_constraintBottom_toBottomOf="@+id/detail_guideline4"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/colorMix"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline5"
            app:layout_constraintBottom_toBottomOf="@+id/detail_guideline5"/>
        <LinearLayout
            android:layout_width="2dp"
            android:layout_height="0dp"
            android:background="@color/colorMix"
            app:layout_constraintLeft_toLeftOf="@+id/detail_guideline34"
            app:layout_constraintRight_toRightOf="@+id/detail_guideline34"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline3"
            app:layout_constraintBottom_toBottomOf="@+id/detail_guideline5"/>

        <LinearLayout
            android:layout_width="2dp"
            android:layout_height="0dp"
            android:background="@color/colorMix"
            app:layout_constraintLeft_toLeftOf="@+id/detail_guideline35"
            app:layout_constraintRight_toRightOf="@+id/detail_guideline35"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline3"
            app:layout_constraintBottom_toBottomOf="@+id/detail_guideline5"/>

        <LinearLayout
            android:layout_width="2dp"
            android:layout_height="0dp"
            android:background="@color/colorMix"
            app:layout_constraintLeft_toLeftOf="@+id/detail_guideline99"
            app:layout_constraintRight_toRightOf="@+id/detail_guideline99"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/detail_guideline2"/>


        <TextView
            android:id="@+id/detail_tx_pric"
            style="@style/MyEdittextStyle6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="bottom"
            android:text="@string/price"
            android:layout_margin="5dp"
            app:layout_constraintBottom_toTopOf="@+id/detail_guideline2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/detail_guideline99" />

        <TextView
            android:id="@+id/detail_tx_pric_dollar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/dollar"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="@color/colorD"
            app:layout_constraintBottom_toTopOf="@+id/detail_tx_pric"
            app:layout_constraintLeft_toLeftOf="@+id/detail_tx_pric"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline1" />


        <TextView
            android:id="@+id/detail_tx_pric_euro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/euro"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/detail_tx_pric"
            android:textColor="@color/colorD"
            app:layout_constraintRight_toRightOf="@+id/detail_tx_pric"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline1" />

        <TextView
            android:id="@+id/detail_tx_surface"
            style="@style/MyEdittextStyle6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="bottom"
            android:text="@string/surface"
            android:layout_margin="5dp"
            app:layout_constraintBottom_toBottomOf="@+id/detail_guideline2"
            app:layout_constraintLeft_toRightOf="@id/detail_guideline99"
            app:layout_constraintRight_toRightOf="parent" />

        <TextView
            android:id="@+id/detail_tx_surface_square"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/sq_ft"
            android:textSize="15sp"
            android:textStyle="bold"
            android:textColor="@color/colorD"
            app:layout_constraintLeft_toRightOf="@+id/detail_tx_surface_m2"
            app:layout_constraintBottom_toTopOf="@+id/detail_tx_surface"
            app:layout_constraintRight_toRightOf="@+id/detail_tx_surface"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline1" />


        <TextView
            android:id="@+id/detail_tx_surface_m2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/m"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintRight_toLeftOf="@+id/detail_tx_surface_square"
            app:layout_constraintBottom_toTopOf="@+id/detail_tx_surface"
            android:textColor="@color/colorD"
            app:layout_constraintLeft_toLeftOf="@+id/detail_tx_surface"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline1" />




        <TextView
            android:id="@+id/detail_adress"
            style="@style/MyEdittextStyle6"
            android:padding="4dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorB"
            android:gravity="bottom"
            android:text="@string/adress"
            app:layout_constraintBottom_toTopOf="@+id/detail_guideline3"
            app:layout_constraintLeft_toLeftOf="@+id/detail_guideline8"
            app:layout_constraintRight_toRightOf="@+id/detail_guideline9" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/location_grey"
            app:layout_constraintLeft_toLeftOf="@+id/detail_adress"
            app:layout_constraintRight_toRightOf="@+id/detail_adress"
            app:layout_constraintBottom_toTopOf="@+id/detail_adress"
            android:contentDescription="@string/location" />

        <TextView
            android:id="@+id/detail_city"
            style="@style/MyEdittextStyle6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="bottom"
            android:text="@string/city"
            android:layout_margin="5dp"
            app:layout_constraintBottom_toTopOf="@+id/detail_guideline4"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/detail_guideline34" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/city_yellow"
            android:layout_margin="5dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/detail_guideline34"
            app:layout_constraintTop_toBottomOf="@+id/detail_guideline3"
            android:contentDescription="@string/city" />


        <TextView
            android:id="@+id/detail_type"
            style="@style/MyEdittextStyle6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@color/colorB"
            android:gravity="bottom"
            android:importantForAutofill="no"
            android:text="@string/type"
            app:layout_constraintBottom_toTopOf="@+id/detail_guideline4"
            app:layout_constraintLeft_toLeftOf="@+id/detail_guideline35"
            app:layout_constraintRight_toRightOf="parent" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/house_grey"
            app:layout_constraintLeft_toLeftOf="@+id/detail_guideline35"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/detail_guideline3"
            android:contentDescription="@string/type" />

        <TextView
            android:id="@+id/detail_personn"
            style="@style/MyEdittextStyle6"
            android:layout_width="wrap_content"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:background="@color/colorB"
            android:elevation="5dp"
            android:gravity="bottom"
            android:importantForAutofill="no"
            android:text="@string/personn"
            app:layout_constraintBottom_toTopOf="@+id/detail_guideline4"
            app:layout_constraintLeft_toRightOf="@+id/detail_guideline34"
            app:layout_constraintRight_toLeftOf="@+id/detail_guideline35"
            tools:targetApi="lollipop" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/person_grey"
            android:elevation="10dp"
            app:layout_constraintLeft_toRightOf="@+id/detail_guideline34"
            android:layout_margin="5dp"
            app:layout_constraintRight_toLeftOf="@+id/detail_guideline35"
            app:layout_constraintTop_toBottomOf="@+id/detail_guideline3"
            tools:targetApi="lollipop"
            android:contentDescription="@string/person" />

        <TextView
            android:id="@+id/detail_datebegin"
            style="@style/MyEdittextStyle6"
            android:layout_width="wrap_content"
            android:layout_margin="5dp"
            android:layout_height="wrap_content"
            android:background="@color/colorB"
            android:gravity="bottom"
            android:importantForAutofill="no"
            android:text="@string/creation_date"
            app:layout_constraintBottom_toTopOf="@+id/detail_guideline5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/detail_guideline34" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/calendar_grey"
            android:layout_margin="5dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/detail_guideline34"
            app:layout_constraintTop_toBottomOf="@+id/detail_guideline4"
            android:contentDescription="@string/creation_date" />

        <TextView
            android:id="@+id/detail_dateend"
            style="@style/MyEdittextStyle6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/colorB"
            android:gravity="bottom"
            android:layout_margin="5dp"
            android:importantForAutofill="no"
            android:text="@string/date_sale"
            app:layout_constraintBottom_toTopOf="@+id/detail_guideline5"
            app:layout_constraintLeft_toLeftOf="@+id/detail_guideline35"
            app:layout_constraintRight_toRightOf="parent" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/check_yellow"
            app:layout_constraintLeft_toLeftOf="@+id/detail_guideline35"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline4"
            android:contentDescription="@string/date_sale" />

        <TextView
            android:id="@+id/detail_room"
            style="@style/MyEdittextStyle6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/colorB"
            android:gravity="bottom"
            android:layout_margin="5dp"
            android:importantForAutofill="no"
            android:text="@string/nb_room"
            app:layout_constraintBottom_toTopOf="@+id/detail_guideline5"
            app:layout_constraintLeft_toRightOf="@+id/detail_guideline34"
            app:layout_constraintRight_toLeftOf="@+id/detail_guideline35" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bed_grey"
            android:layout_margin="5dp"
            app:layout_constraintLeft_toRightOf="@+id/detail_guideline34"
            app:layout_constraintRight_toLeftOf="@+id/detail_guideline35"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline4"
            android:contentDescription="@string/nb_room" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline71"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="@+id/detail_map">

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/middle_cl"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.5" />


            <TextView
                android:id="@+id/detail_description"
                style="@style/MyEdittextStyle6"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:ellipsize="marquee"
                android:gravity="start"
                android:scrollbars="vertical"
                android:justificationMode="inter_word"
                android:maxLength="1000"
                android:maxLines="10"
                android:text="@string/description"
                android:textAlignment="gravity"
                android:layout_margin="10dp"
                app:layout_constraintTop_toBottomOf="@+id/detail_icon_desc"
                app:layout_constraintRight_toRightOf="@+id/middle_cl"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:targetApi="o" />

            <ImageView
                android:id="@+id/detail_icon_desc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@drawable/info_yellow"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:contentDescription="@string/description" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageView
            android:layout_margin="2dp"
            android:id="@+id/detail_map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/square_yellow"
            android:padding="2dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@+id/detail_guidelineC"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/detail_guideline71"
            app:layout_goneMarginBottom="32dp"
            android:contentDescription="@string/map" />

        <TextView
            android:background="@drawable/round_yellow"
            style="@style/MyEdittextStyle6"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="@string/click_for_more_information"
            android:textColor="@color/colorB"
            android:textSize="@dimen/textadaptsizesmall"
            app:layout_constraintBottom_toTopOf="@+id/im_guideline5"
            app:layout_constraintTop_toTopOf="@id/detail_map"
            app:layout_constraintLeft_toLeftOf="@id/detail_map"
            app:layout_constraintRight_toRightOf="@id/detail_map" />
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.15" />
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.35" />

        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.55" />

        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.75" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/im_guideline5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.78" />

        <LinearLayout
            android:id="@+id/LinearLayout1"
            app:layout_constraintRight_toLeftOf="@+id/LinearLayout2"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline7"
            app:layout_constraintLeft_toLeftOf="parent"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/hospital_yellow"
                android:backgroundTint="@color/colorD"
                tools:targetApi="lollipop"
                android:contentDescription="@string/hospital" />
            <TextView
                style="@style/EditTXTCreate"
                android:layout_marginStart="5dp"
                android:id="@+id/detail_pharmacy_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/empty" />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/LinearLayout2"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline7"
            app:layout_constraintLeft_toRightOf="@+id/LinearLayout1"
            app:layout_constraintRight_toLeftOf="@+id/LinearLayout3"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/school_yellow"
                android:backgroundTint="@color/colorD"
                tools:targetApi="lollipop"
                android:contentDescription="@string/school"/>
            <TextView
                style="@style/EditTXTCreate"
                android:layout_marginStart="5dp"
                android:id="@+id/detail_school_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/empty"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout3"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline7"
            app:layout_constraintLeft_toRightOf="@+id/LinearLayout2"
            app:layout_constraintRight_toLeftOf="@+id/LinearLayout4"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/market_yellow"
                android:backgroundTint="@color/colorD"
                tools:targetApi="lollipop"
                android:contentDescription="@string/market" />
            <TextView
                style="@style/EditTXTCreate"
                android:layout_marginStart="5dp"
                android:id="@+id/detail_market_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/empty"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout4"
            app:layout_constraintTop_toTopOf="@+id/detail_guideline7"
            app:layout_constraintLeft_toRightOf="@+id/LinearLayout3"
            app:layout_constraintRight_toRightOf="parent"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/park_green"
                android:backgroundTint="@color/colorD"
                tools:targetApi="lollipop"
                android:contentDescription="@string/park" />
            <TextView
                style="@style/EditTXTCreate"
                android:layout_marginStart="5dp"
                android:id="@+id/detail_park_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/empty" />
        </LinearLayout>
        <com.sofianem.realestatemanager.controller.view.HorizontalCarouselRecyclerView
            android:id="@+id/item_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clipToPadding="false"
            android:overScrollMode="never"
            app:layout_constraintBottom_toBottomOf="@+id/detail_guideline7"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/detail_guideline5"/>

    </androidx.constraintlayout.widget.ConstraintLayout>



    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/detail_sold"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorGreyTransp"
        android:orientation="horizontal"
        android:visibility="gone">
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.4" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/soldguideline"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.1" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/soldguideline2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.2" />
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.1" />
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.9" />
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.05" />
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.25" />



        <LinearLayout
            android:id="@+id/llSold"
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/soldguideline2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:background="@drawable/rectangle_yellow"
            android:layout_margin="10dp"
            app:layout_constraintTop_toTopOf="@+id/soldguideline"/>

        <TextView
            android:rotation="-10"
            app:layout_constraintTop_toTopOf="@+id/llSold"
            app:layout_constraintBottom_toBottomOf="@+id/llSold"
            app:layout_constraintLeft_toLeftOf="@+id/llSold"
            app:layout_constraintRight_toRightOf="@+id/llSold"
            android:textColor="@color/colorB"
            style="@style/MyEditTextStyle3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/sold"
            android:textSize="40sp"
            android:visibility="visible" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>